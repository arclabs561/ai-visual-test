{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.mjs"],
  "entryPointStrategy": "resolve",
  "out": "./docs-generated",
  "name": "ai-browser-test",
  "readme": "./README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "sort": ["source-order", "required-first", "kind"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": false
  },
  "categorizeByGroup": false,
  "searchCategoryBoosts": {
    "Core": 2.0,
    "Utilities": 1.5
  },
  "searchGroupBoosts": {
    "Classes": 1.5,
    "Functions": 1.2
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "jsDocCompatibility": {
    "exampleTag": true,
    "defaultTag": true,
    "inheritDocTag": true,
    "ignoreUnescapedBraces": true
  },
  "projectDocuments": [
    "docs/API_ESSENTIALS.md",
    "docs/PRIMARY_API.md"
  ],
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false
  }
}

